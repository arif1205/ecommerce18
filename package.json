{
  "name": "shopaholic",
  "version": "1.0.0",
  "description": "shopping cart",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "bankserver": "nodemon bankbackend/server",
    "supplierserver": "nodemon supplierbackend/server",
    "client": "npm start --prefix frontend",
    "bankclient": "npm start --prefix bankfrontend",
    "dev": "concurrently \"npm run server\" \"npm run bankserver\" \"npm run supplierserver\" \"npm run client\" \"npm run bankclient\"",
    "data:import": "node backend/seeder",
    "data:importbank": "node bankbackend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nowshadjunaed/Shopaholic.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nowshadjunaed/Shopaholic/issues"
  },
  "homepage": "https://github.com/Nowshadjunaed/Shopaholic#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "easyinvoice": "^2.3.3",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jspdf-invoice-template": "^1.4.3",
    "mongoose": "^6.3.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}
